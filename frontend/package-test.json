{
  "name": "ai-lawyer-frontend-testing",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "cypress": "^13.6.0",
    "cypress-axe": "^1.5.0",
    "cypress-real-events": "^1.11.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-axe": "^8.0.0",
    "lighthouse": "^11.4.0",
    "puppeteer": "^21.6.1",
    "webpack-bundle-analyzer": "^4.10.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-jsx-a11y": "^6.8.0"
  },
  "scripts": {
    "test:unit": "react-scripts test --watchAll=false --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:performance": "node scripts/performance-tests.js",
    "test:security": "npm audit && eslint --ext .js,.jsx src/ --config .eslintrc.security.js",
    "test:accessibility": "cypress run --spec 'cypress/e2e/accessibility/**/*'",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:performance && npm run test:security"
  }
}